{
  "expo": {
    "name": "Daily Ups",
    "slug": "DailyUps",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/finalLogo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/finalLogo.png",
      "resizeMode": "contain",
      "backgroundColor": "#1a1e21"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/finalLogo.png",
        "backgroundColor": "#1a1e21"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
